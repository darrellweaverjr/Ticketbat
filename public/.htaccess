    <IfModule mod_negotiation.c>
        Options -MultiViews
    </IfModule>

    <IfModule mod_headers.c>
       RequestHeader unset Proxy
    </IfModule>


#   Options +SymLinksIfOwnerMatch
    RewriteEngine On

#   Blocking traffic to /autodiscover/autodiscover.xml
    RewriteCond %{REQUEST_URI} /autodiscover/autodiscover.xml
    RewriteRule ^ - [F,L]

#   Blocking urls to old uploads folder for images
    RewriteRule (^|/)uploads(/|$) - [F]

#   Anti Hacker
    RewriteCond %{REQUEST_URI} /wp-login.php [NC]
    RewriteRule ^ - [F,L]
    RewriteCond %{REQUEST_URI} /administrator/manifests/files/joomla.xml [NC]
    RewriteRule ^ - [F,L]
    RewriteCond %{REQUEST_URI} /wp-settings.php/index.php [NC]
    RewriteRule ^ - [F,L]
    RewriteCond %{REQUEST_URI} /api/.DS_Store [NC]
    RewriteRule ^ - [F,L]
    RewriteCond %{REQUEST_URI} /api/info.php [NC]
    RewriteRule ^ - [F,L]
    RewriteCond %{REQUEST_URI} /admin/phpinfo.php [NC]
    RewriteRule ^ - [F,L]
    RewriteCond %{REQUEST_URI} /js/admin/commands/axis/DirectDownload.jsp [NC]
    RewriteRule ^ - [F,L]
    RewriteCond %{REQUEST_URI} /phpMyAdmin/scripts/setup.php [NC]
    RewriteRule ^ - [F,L]
    RewriteCond %{REQUEST_URI} /phpMyAdmin/scripts/_setup.php [NC]
    RewriteRule ^ - [F,L]
    RewriteCond %{REQUEST_URI} /MyAdmin/scripts/setup.php [NC]
    RewriteRule ^ - [F,L]
    RewriteCond %{REQUEST_URI} /pma/scripts/setup.php [NC]
    RewriteRule ^ - [F,L]
    RewriteCond %{REQUEST_URI} /cgi-sys/suspendedpage.cgi [NC]
    RewriteRule ^ - [F,L]
    RewriteCond %{REQUEST_URI} /scripts/setup.php [NC]
    RewriteRule ^ - [F,L]
    RewriteCond %{REQUEST_URI} /admin/scripts/setup.php [NC]
    RewriteRule ^ - [F,L]
    RewriteCond %{REQUEST_URI} /dbadmin/scripts/setup.php [NC]
    RewriteRule ^ - [F,L]
    RewriteCond %{REQUEST_URI} /mysql/scripts/setup.php [NC]
    RewriteRule ^ - [F,L]

#   Force connections to https in ticketbat.com public site
    RewriteCond %{HTTP_HOST} ^ticketbat\.com [NC]
    RewriteCond %{SERVER_PORT} 80 
    RewriteRule ^(.*)$ https://www.ticketbat.com/$1 [R,L]

#   Redirect Trailing Slashes If Not A Folder...
    RewriteCond %{REQUEST_FILENAME} !-d
    RewriteRule ^(.*)/$ /$1 [L,R=301]

#   Handle Front Controller...
    RewriteCond %{REQUEST_FILENAME} !-d
    RewriteCond %{REQUEST_FILENAME} !-f
    RewriteRule ^ index.php [L]

#   Handle Robots.txt for the live site
    RewriteRule ^robots.txt - [L]

#   These rules below after all the [L] last rules
#   Handle Authorization Header
    RewriteCond %{HTTP:Authorization} .
    RewriteRule .* - [E=HTTP_AUTHORIZATION:%{HTTP:Authorization}]

#   Disable view files in the browser inside folder public, except from this urls
    RewriteCond %{HTTP_REFERER} !^http://(www\.)?localhost [NC] 
    RewriteCond %{HTTP_REFERER} !^https://(www\.)?localhost [NC] 
    RewriteCond %{HTTP_REFERER} !^http://(www\.)?admin.ticketbat.com [NC] 
    RewriteCond %{HTTP_REFERER} !^https://(www\.)?admin.ticketbat.com [NC] 
    RewriteCond %{HTTP_REFERER} !^http://(www\.)?staging.ticketbat.com [NC]
    RewriteCond %{HTTP_REFERER} !^https://(www\.)?staging.ticketbat.com [NC]
    RewriteCond %{HTTP_REFERER} !^http://(www\.)?ticketbat.com [NC]
    RewriteCond %{HTTP_REFERER} !^https://(www\.)?ticketbat.com [NC]
    RewriteRule \.(gif|png|txt|psd|eot|svg|ttf|woff|woff2|js|css)$ - [F]




